{
  "name": "@openmev/utils",
  "version": "0.2.2",
  "description": "OpenMEV SDK",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.esm.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm build:cjs && npm build:esm",
    "build:cjs": "npx tse build src/index.ts --emit-types --ext .cjs.js",
    "build:esm": "npx tse build src/index.ts --emit-types --ext .esm.js --output esm",
    "lint": "npx tse lint .",
    "test": "npx tse test --coverage",
    "gen-index": "npx ctix create tsconfig.json"
  },
  "keywords": [
    "ethereum",
    "defi",
    "solidity",
    "sdk",
    "mev",
    "blockchain"
  ],
  "author": "Manifold Finance, Inc <janitor@manifoldfinance.com>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifoldfinance/openmev-sdk.git",
    "directory": "packages/utils"
  },
  "devDependencies": {
    "@ethersproject/bignumber": "^5.4.2",
    "@ts-engine/cli": "^2.0.0",
    "@types/chai": "^4.2.22",
    "@types/long": "^4.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.21",
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "ctix": "^0.3.1",
    "grpc-tools": "^1.11.2",
    "jsbi": "^3.2.4",
    "long": "^4.0.0",
    "mocha": "^9.1.2",
    "protobufjs": "^6.11.2",
    "ts-loader": "^9.2.5",
    "ts-node-dev": "^1.1.8",
    "ts-proto": "^1.82.5",
    "tslib": "^2.3.1",
    "typedoc": "^0.20.23",
    "typedoc-plugin-remove-references": "^0.0.5",
    "typedoc-plugin-sourcefile-url": "^1.0.6",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.4.1",
    "@ethersproject/providers": "^5.4.5",
    "@ethersproject/strings": "^5.4.0"
  },
  "gitHead": "fc44fce7e2b0df20cca193a5210929ffca1ad725"
}
